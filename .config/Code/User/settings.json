{
    "files.autoSave": "afterDelay",
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "git.autofetch": true,
    "terminal.integrated.defaultProfile.linux": "fish",
    "git.openRepositoryInParentFolders": "always",
    "java.import.gradle.java.home": "/run/current-system/sw/lib/openjdk/",
    "java.jdt.ls.java.home": "/run/current-system/sw/lib/openjdk/",
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "editor.tabSize": 2,
    "editor.rulers": [
        80,
        120
    ],
    "workbench.iconTheme": "material-icon-theme",
    "indentRainbow.indicatorStyle": "light",
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#88c0d0",
        "editorBracketHighlight.foreground2": "#81a1c1",
        "editorBracketHighlight.foreground3": "#5e81ac",
        "editorBracketHighlight.foreground4": "#88c0d0",
        "editorBracketHighlight.foreground5": "#81a1c1",
        "editorBracketHighlight.foreground6": "#5e81ac",
    },
    "indentRainbow.colors": [
        "rgba(136, 192, 208, .5)",
        "rgba(129, 161, 193, .5)",
        "rgba(94, 129, 172, .5)",
    ],
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "window.titleBarStyle": "native",
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorTheme": "Nord Deep",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "github.copilot.enable": {
        "*": false,
        "plaintext": false,
        "markdown": false,
        "scminput": false
    },
    "[dockercompose]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 2,
        "editor.autoIndent": "advanced",
        "editor.defaultFormatter": "redhat.vscode-yaml"
    },
    "[github-actions-workflow]": {
        "editor.defaultFormatter": "redhat.vscode-yaml"
    }
}